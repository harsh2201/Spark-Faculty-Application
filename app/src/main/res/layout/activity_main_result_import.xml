<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/new_vectr"
    tools:context=".Main_result_import">

    <Spinner
        android:id="@+id/SubjectSpinnerImport"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/shape"
        android:elevation="13dp"
        android:textAlignment="center" />

    <Button
        android:id="@+id/ImprtButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/SubjectSpinnerImport"
        android:layout_centerHorizontal="true"
        android:background="@drawable/shape"
        android:text="Import Excel"
        android:layout_marginTop="50dp"
        android:textColor="@color/white" />

    <Button
        android:id="@+id/LogoutButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ImprtButton"
        android:layout_centerHorizontal="true"
        android:background="@drawable/shape"
        android:text="      Log Out     "
        android:textAlignment="center"
        android:layout_marginTop="30dp"
        android:textColor="@color/white" />

</RelativeLayout>